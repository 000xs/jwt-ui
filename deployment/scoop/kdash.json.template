{
  "homepage": "https://github.com/jwt-rs/jwt-tui",
  "description": "A fast and simple dashboard for Kubernetes",
  "version": "$version64",
  "license": "MIT",
  "architecture": {
    "64bit": {
      "url": "https://github.com/jwt-rs/jwt-tui/releases/download/v$version64/jwt-tui-windows.tar.gz",
      "hash": "$hash_64"
    }
  },
  "bin": "jwt-tui.exe",
  "checkver": "github",
  "autoupdate": {
    "architecture": {
      "64bit": {
        "url": "https://github.com/jwt-rs/jwt-tui/releases/download/v$version/jwt-tui-windows.tar.gz",
        "hash": {
          "url": "https://github.com/jwt-rs/jwt-tui/releases/download/v$version/jwt-tui-windows.sha256",
          "regex": "$sha256"
        }
      }
    }
  }
}
